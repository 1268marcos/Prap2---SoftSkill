import androidx.annotation.NonNull;
import androidx.annotation.Nullable;
import androidx.room.ColumnInfo;
import androidx.room.Entity;
import androidx.room.PrimaryKey;

@Entity(tableName = "softs")

public class SoftSkill {




private String nCreationDate;

private String nLearning;
private int nLevel;

 public SoftSkill (@Nullable String name, String learning, int level, String CreationDate) {

        nName = name;
        nLearning = learning;
        nLevel = level;
        nCreationDate = CreationDate;
    }

@PrimaryKey ( autoGenerate = true ) 
   @NonNull
@ColumnInfo ( name ="SoftskillId" )
private int nId;



@nullable
@ColumnInfo ( name = "nName" )
private String nName;

public int getId () {
return nId;
}

public void setId ( int id ) {
nId = id;
}

public String getName () {
return nName;
}

 public void setName (String nome){
nName = nome; public void setName (String nome){
}

public String getLearning () {
return nLearning;
}

public void setLearning (String learning){
            nLearning = learning;
        }

  public int getLevel () {
            return nLevel;
        }

public void setLevel ( int level ){
            nLevel = level;
        }

public String getCreationDate () {
return nCreationDate;

}

public void setCreationDate ( String CreationDate ) {
nCreationDate = nCreationDate;
}


}

//fim da classe

}
